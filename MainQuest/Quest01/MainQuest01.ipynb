        "import random\n",
        "\n",
        "class History:\n",
        "    count = 0\n",
        "\n",
        "    def __init__(self, type, money, account):\n",
        "        self.type = type\n",
        "        self.money = money\n",
        "        self.account = account\n",
        "        self.count = History.get_next_id()\n",
        "\n",
        "    @classmethod\n",
        "    def get_next_id(cls):\n",
        "        cls.count += 1\n",
        "        return cls.count\n",
        "\n",
        "class Account: # Q1 Account 클래스\n",
        "    acc_count = 0 # Q2 계좌 객체의 개수를 저장하는 클래스 변수\n",
        "\n",
        "    def __init__(self, name, account):\n",
        "        self.bank = \"SC은행\"\n",
        "        self.acc_num = str(random.randint(0,999)).zfill(3) + '-' + str(random.randint(0,99)).zfill(2) + '-' + str(random.randint(0,999999)).zfill(6) # 계좌번호\n",
        "        self.name = name\n",
        "        self.account = account\n",
        "\n",
        "        self.history = [] # 입출금 내역\n",
        "        self.dep_count = 0 # 이자 count\n",
        "        Account.acc_count += 1\n",
        "\n",
        "    @classmethod\n",
        "    def get_account_num(cls): #Q3 계좌의 개수를 출력하는 메서드\n",
        "        print(f\"생성된 계좌의 총 개수: {cls.acc_count}\\n\")\n",
        "\n",
        "    def deposit(self, add_money): # Q4 입금 메서드\n",
        "        if add_money < 1:\n",
        "            print(\"입금은 최소 1원 이상만 가능합니다.\\n\")\n",
        "        else:\n",
        "            self.account += add_money\n",
        "            self.dep_count += 1\n",
        "\n",
        "            self.history.append(History(\"입금\", add_money, self.account))\n",
        "            if self.dep_count % 5 == 0: # Q7 입금 횟수가 5회째일때 마다 이자 지급\n",
        "                intrest = int(self.account * 0.01)\n",
        "                self.account += intrest\n",
        "                print(f\"{intrest}의 이자가 입금되었습니다.\\n\")\n",
        "                self.history.append(History(\"이자지급\", intrest, self.account))\n",
        "\n",
        "    def withdraw(self, sub_money): # Q5 출금 메서드\n",
        "        if sub_money > self.account:\n",
        "            print(\"계좌 잔고 이상으로 출금할 수 없습니다.\\n\")\n",
        "        else:\n",
        "            self.account -= sub_money\n",
        "            self.history.append(History(\"출금\", sub_money, self.account))\n",
        "\n",
        "    def display_info(self): # Q6 정보 출력 메서드\n",
        "        print(f\"은행이름: {self.bank}, 예금주: {self.name}, 계좌번호: {self.acc_num}, 잔고: {format(self.account,',')}원\")\n",
        "\n",
        "    # Q10 입출금 내역\n",
        "    def deposit_history(self):\n",
        "        for i in self.history:\n",
        "            if i.type != \"출금\":\n",
        "                print(\"{0:^40}\".format(f\"{i.count}회: {i.type}       금액: {i.money}     잔액: {i.account}\"))\n",
